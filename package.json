{
  "name": "@versoly/eslint-config",
  "version": "1.0.3",
  "description": "ESLint + Prettier config personalizada da Versoly",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "keywords": ["eslint", "prettier", "config"],
  "author": "Wesley Rodio / @WesleyRodio",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.8.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "prettier": "^3.5.3"
  },
  "scripts": {
    "postinstall": "cp ./node_modules/@versoly/eslint-config/.prettierrc .prettierrc"
  },
  "eslintConfig": {
    "extends": "@versoly/eslint-config"
  },
  "prettier": "@versoly/eslint-config/.prettierrc"
